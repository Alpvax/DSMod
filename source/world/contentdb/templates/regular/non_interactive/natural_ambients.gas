[t:template,n:base_ambient]
{
	category_name = "ambients";
	doc = "Base template for all non-interactive ambients";
	specializes = non_interactive_nonblocking;
	
	[aspect]
	{
		draw_selection_indicator = false;
		is_selectable = true;
		lodfi_lower = 0;
		lodfi_upper = 0;
	}
	common:rollover_display = true;
}
[t:template,n:cow_black_white_mp]
{
	category_name = "1w_ambients";
	doc = "MP Black Cow";
	specializes = base_ambient;
	[aspect]
	{
		model = m_c_na_cw_pos_1;
		
	}
	[body]
	{
		initial_chore = chore_default;
		[chore_dictionary]
		{
			chore_prefix = a_c_na_cw_fs0;
			[chore_default]
			{
				skrit = random_infinite_loop;
				[anim_files]
				{
					00 = gz;
				}
			}
		}
	}
	[common] { screen_name="cow"; }
}
