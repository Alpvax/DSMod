//
//	Monster related effect scripts
//
//	Any special Monster effects belong here
//	

[effect_script*]
{
	name	=ice_fly;

	script	=[[
				sfx create fire #TARGET
			"ts(.75)count(20)scale(1)color0(.2,.3,.5)velocity(0,0,0)
			accel(0,-2,0)alphafade(.5)";
			
				
			sfx attach_point #PEEK bone01 target;
			set $flame1 #POP;
			sfx start $flame1;
			
			sfx create fire #TARGET
			"flamesize(.4)texture(b_sfx_sparkle01)ts(.75)count(20)
			scale(1)color0(.2,.3,.5)velocity(0,0,0)accel(0,-5,0)
			alphafade(.5)";
			
				
			sfx attach_point #PEEK bone02 target;
			set $flame2 #POP;
			sfx start $flame2;
			
			sfx create fire #TARGET
			"flamesize(.4)texture(b_sfx_sparkle01)ts(.75)count(20)
			scale(1)color0(.2,.3,.5)velocity(0,0,0)accel(0,-5,0)
			alphafade(.5)";
			
				
			sfx attach_point #PEEK bone03 target;
			set $flame3 #POP;
			sfx start $flame3;

			sfx create linetracer #TARGET 
				"color0(.5,.6,1)color1(.2,.3,1)fade_rate(3)";
			set $trace #POP;
			sfx target $trace target;
			sfx attach_point $trace bone11 target;
			sfx attach_point $trace bone01 source;
			
			sfx start $trace;

			]];

}
[effect_script*]
{
