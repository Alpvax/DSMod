[effect_script*]
{
	name	=fury_charge;

	script	=[[
			if ( [0] == 0 )
			{
				set $AP AP_right3; 
				set $color 1,.5,.5;
			}

			if ( [0] == 1 )
			{
				set $AP AP_right2; 
				set $color .5,1,.5;
			}

			if ( [0] == 2 )
			{
				set $AP AP_right1;
				set $color .5,.5,1;
			}

			if ( [0] == 3 )
			{
				set $AP AP_left3;
				set $color 1,.5,1;
			}

			if ( [0] == 4 )
			{
				set $AP AP_left2;
				set $color 1,1,.5;
			}

			if ( [0] == 5 )
			{
				set $AP AP_left1; 
				set $color .5,1,1;
			}

			sfx create charge #TARGET
			"ts(2.5)dur(.5)color0($color)count(20)tout(.2)";
			
			
			sfx attach_point #PEEK $AP target;
			sfx start #POP;
			
			]];
}

