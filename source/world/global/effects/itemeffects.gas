	name	=steal_drip_15_or;

	script	=[[
			if( !(#WEAPON_TYPE_DAGGER || #WEAPON_TYPE_AXE || #WEAPON_TYPE_BLUNT || #WEAPON_TYPE_STAFF || #WEAPON_TYPE_SWORD) )
			{
				exit;
			}
			
			set $texture b_sfx_sparkle01;

			sfx create orbiter #TARGET
				"must_update()scale(.2)color0(.7,.4,.1)radius(0)";
			sfx attach_point #PEEK AP_trace01 target;
			sfx start #POP;

			sfx create orbiter #TARGET
				"must_update()scale(.2)color0(.7,.4,.1)radius(0)";
			sfx attach_point #PEEK AP_trace02 target;
			sfx start #POP;

			sfx create lightning #TARGET
				"must_update()scale(1)subd(.67)minsubd(.67)color0(.7,.4,.1)mindisplace(0)
				maxdisplace(0)";
			sfx target #PEEK target;
			sfx attach_point #PEEK AP_trace01 target;
			sfx attach_point #PEEK AP_trace02 source;
			sfx start #POP;

			sfx create explosion #TARGET
				"must_update()collide(1)fade_range(1,1,0)texture($texture)count(30)
				line()ts(1)color0(.7,.4,.1)scale(.5)srate(.015)";
			sfx target #PEEK target;
			sfx attach_point #PEEK AP_trace01 target;
			sfx attach_point #PEEK AP_trace02 source;
			sfx offset_bone #PEEK v<-.06 -.1 0> source;
			sfx offset_bone #PEEK v<-.06 -.1 0> target;
			sfx start #POP;
			]];
}
