[effect_script*]
{
	name	=tenstone_decay;

	script	=[[
	
			pause 1;
			
			sfx create explosion #TARGET
				"srate(.01)ts(.5)radius(.1)color0(1,.2,.1)color1(0,0,0)
				ivel(0,0,0)rvel(1.7,0,1.7)vmin(2.5)vmax(2.7)count(12)
				rand_scale()scale_range(.16,.18,0)fade_range(4,4,0)radius(1)";
			sfx start #POP;
			
			pause 1;
			
			sfx create fire #TARGET 
				"scale(.66)max_radius(2)max_displace(.1)sinspeed(.002)
				sinpos(0)count(60)velocity(0,10,0)accel(0,10,0)color1(0,0,0)
				alphafade(1)flamesize(.2)dark()grey_tex()color0(1,.2,.1)";
			sfx start #POP;
			
			sfx create fire #TARGET 
				"scale(.66)max_radius(2)max_displace(.1)sinspeed(.002)
				sinpos(1.57)count(60)velocity(0,10,0)accel(0,10,0)color1(0,0,0)
				alphafade(1)flamesize(.2)dark()grey_tex()color0(1,.2,.1)";
			sfx start #POP;

			sfx create cylinder #TARGET
				"alpha(.6)hp0(.08,.08,0)hp1(.08,.08,0)rp0(0,1.6,.01)rp1(0,0,0)
				tin(2)tout(2)color0(.1,.1,.1)segments(22)dark()spin(1.2)";
			sfx start #POP;
			]];
}



[effect_script*]
{
	name	=minigun_flamethrower;

	script	=[[
			sound play s_e_attack_igniter at #SOURCE_POSITION dist 10 30;
			
			set $color .86,.4,.1;
			set $shot_life [0];
			//set $velocity 0,20,200;
			//set $accel 0,20,200;
			set $width1 0;
			set $width2 0;
			
			set $velocity 100,0,0;
			set $accel 0,0,0;

			set $flames1 60;
			set $flames2 80;

			sfx create fireb #SOURCE 
				"must_update()fctrl(0,0,7)flamesize(1)alphafade(.8)ts(.5)color0(.85,.6,.6)
				velocity($velocity)accel($accel)cvel(1)lower_r0(0)lower_r1(0)dur($shot_life)
				upper_r0(0)upper_r1(30)count(80)bone_orient()texture(b_sfx_fireball-01)
				";
			sfx attach_point #PEEK ap_ammo target;
			sfx friendly party #PEEK;
			sfx start #POP;

			/// texture(b_sfx_splotches_02)
			sfx create fireb #SOURCE 
				"must_update()fctrl(0,0,7)flamesize(2)alphafade(.8)ts(.5)color0(.85,.6,.6)
				velocity($velocity)accel($accel)cvel(1)lower_r0(0)lower_r1(0)start_rate(10)dur($shot_life)
				upper_r0(20)upper_r1(60)count(30)bone_orient()texture(b_sfx_fireball-02)
				";
			sfx attach_point #PEEK ap_ammo target;
			sfx friendly party #PEEK;
			sfx start #POP;

			sfx create fireb #SOURCE 
				"must_update()fctrl(0,0,7)flamesize(2)alphafade(.8)ts(.5)color0(1,.3,0)
				velocity($velocity)accel($accel)cvel(1)lower_r0(1)lower_r1(1)dur($shot_life)
				upper_r0(40)upper_r1(90)count(80)bone_orient()";
			sfx attach_point #PEEK ap_ammo target;
			sfx friendly party #PEEK;
			sfx start #POP;
			
			sfx create fireb #SOURCE 
				"must_update()fctrl(0,0,2)flamesize(0)alphafade(.75)ts(.5)color0(1,.9,.8)
				velocity($velocity)accel($accel)cvel(1)lower_r0(0)lower_r1(0)dur($shot_life)
				upper_r0(70)upper_r1(90)count(80)texture(b_sfx_sparkle01)offset(-.5,0,0)[1]
				bone_orient()min_displace(-3)max_displace(3)";
			sfx attach_point #PEEK ap_ammo target;
			sfx friendly party #PEEK;
			sfx start #POP;

			worldmsg WE_SPELL_SYNC_END #INVALID_GOID #OWNER_GOID #TARGET_GOID;

			]];
}

[effect_script*]
{
	name	=minigun_lightning;

	script	=[[
			sound play s_e_minigun_lightning at #SOURCE_POSITION dist 10 30;
			
			set $color .5,.5,1;
			set $shot_life [0];
			set $velocity 0,20,200;
			set $accel 0,20,200;
			set $width1 0;
			set $width2 0;
			
			sfx create explosion #TARGET_KB 
				"srate(.01)vmin(2)vmax(3)omni_dir()ivel(0,0,0)rvel(2,0,2)
				scale_range(.3,.3,0)texture(b_sfx_sparkle01)color0(.7,.7,1)
				color1(0,0,0)dur(.5)radius(.2)fade_range(3,3,0)
				count(16)";
			sfx start #POP;
			
			sfx create explosion #SOURCE 
				"srate(.01)vmin(2)vmax(3)omni_dir()ivel(0,0,0)rvel(2,0,2)
				scale_range(.3,.3,0)texture(b_sfx_sparkle01)color0(.7,.7,1)
				color1(0,0,0)dur(.5)radius(.2)fade_range(3,3,0)
				count(16)";
			sfx attach_point #PEEK ap_ammo target;
			sfx start #POP;
			
			sfx create fire #SOURCE 
				"alphafade(.4)flamesize(.6)ts(.8)velocity(0,0,0)accel(0,-4,0)
				dur(.5)scale(1)color0(0.3,0.3,0.6)color1(0,0,0)max_radius(2)";
			sfx attach_point #PEEK ap_ammo target;
			sfx start #POP;
			
			sfx create explosion #SOURCE 
				"srate(.01)vmin(2)vmax(3)omni_dir()ivel(0,0,0)rvel(2,0,2)
				scale_range(.3,.3,0)texture(b_sfx_sparkle01)color0(.7,.7,1)
				color1(0,0,0)dur(.5)radius(.2)fade_range(3,3,0)
				count(16)";
			sfx attach_point #PEEK ap_ammo target;
			sfx start #POP;
			
			sfx create lightning #SOURCE 
				"must_update()scale(1)mindisplace(-.1)maxdisplace(.1)dur(.8)color0($color)";
			sfx attach_point #PEEK ap_ammo target;
			sfx target #PEEK target;
			sfx offset_bone #PEEK v<0 .12 0> source;
			sfx start #POP;
			
			sfx create curve #SOURCE 
				"must_update()ts(2)size(.2)curvature(0.66)model(0)scale(.25)color0($color)
				tlength(15)spacing(4)velocity(50)";
			sfx attach_point #PEEK ap_ammo target;
			sfx target #PEEK target;
			sfx offset_bone #PEEK v<0 .12 0> source;
			sfx start #POP;
			
			sfx create curve #SOURCE 
				"must_update()delay(.1)ts(2)size(.2)curvature(0.35)model(1)scale(.25)
				color0($color)tlength(15)spacing(4)velocity(50)";
			sfx attach_point #PEEK ap_ammo target;
			sfx target #PEEK target;
			sfx offset_bone #PEEK v<0 .12 0> source;
			sfx start #POP;
			
			sfx create curve #SOURCE 
				"must_update()delay(.2)ts(2)size(.2)curvature(0.66)model(0)scale(.25)
				color0($color)tlength(15)spacing(4)velocity(50)";
			sfx attach_point #PEEK ap_ammo target;
			sfx target #PEEK target;
			sfx offset_bone #PEEK v<0 .12 0> source;
			sfx start #POP;
			
			sfx create curve #SOURCE 
				"must_update()delay(.3)ts(2)size(.2)curvature(0.35)model(1)scale(.25)
				color0($color)tlength(15)spacing(4)velocity(50)";
			sfx attach_point #PEEK ap_ammo target;
			sfx target #PEEK target;
			sfx offset_bone #PEEK v<0 .12 0> source;
			sfx start #POP;
			
			worldmsg WE_SPELL_SYNC_MID #INVALID_GOID #OWNER_GOID #TARGET_GOID;
	
			pause $shot_life;

			worldmsg WE_SPELL_SYNC_END #INVALID_GOID #OWNER_GOID #TARGET_GOID;

			]];
}

[effect_script*]
{
	name	=minigun_muzzle_flash;

	script	=[[
		 	sfx create explosion #SOURCE 
				"dir(0,0,1)must_update()texture(b_sfx_fireball-01)radius(.2)color0(1,.8,.6)
				color1(0,.2,.2)vmin(10)vmax(20)srate(.05)fade_range(6,6,0)
				scale(.166)ts(.66)rvel(12,12,12)dur(.66)";
			sfx attach_point #PEEK AP_ammo target;
			sfx start #POP;
		 			 	
		 	sfx create fireb  #SOURCE 
			 	"cvel()start_rate(66)offset(0,0,0)dur(.5)velocity(0,25,0)
			 	scale(1)color0(.4,.4,.4)alphafade(2)lower_r0(.5)lower_r1(.5)
			 	upper_r0(0)upper_r1(0)count(40)fctrl(0,.13,-1.66)flamesize(0.13)
			 	bone_orient()texture(b_sfx_fireball-01)ts(5)";
			sfx attach_point #PEEK AP_ammo target;
			sfx start #POP;
			
			sfx create fireb  #SOURCE 
				"start_rate(12)cvel()dark()dur(.5)velocity(-13,-2,0)accel(6,5,0)
				scale(1)color0(0,0,0)alphafade(1)lower_r0(0)lower_r1(0)
				upper_r0(10)upper_r1(10)count(20)fctrl(-.1,.1,3)flamesize(.1)texture(b_sfx_smoke)";
			sfx attach_point #PEEK bone_muzzle target;
			sfx offset_bone #PEEK v<0 .1 -.15> target;
			sfx start #POP;

			sfx create fireb  #SOURCE 
				"start_rate(12)cvel()dark()dur(.5)velocity(13,-2,0)accel(-6,5,0)
				scale(1)color0(0,0,0)alphafade(1)lower_r0(0)lower_r1(0)
				upper_r0(10)upper_r1(10)count(20)fctrl(-.1,.1,3)flamesize(.1)texture(b_sfx_smoke)";
			sfx attach_point #PEEK bone_muzzle target;
			sfx offset_bone #PEEK v<0 .1 -.15> target;
			sfx start #POP;
			
			frandrange 0 1;
			set $switch #POP;
			
			sfx create explosion #TARGET_KB 
				"count(15)srate(0.02)ts(1)omni_dir()dur(1)vmin(10)vmax(3)
				texture(b_sfx_sparkle01)color0(1,.8,.6)color1(0,.2,.2)
				radius(.1)scale(.66)";
			sfx start #POP;
			
			sound play s_e_minigun_fire at #SOURCE_POSITION;
			set $sound #POP;
			
			pause .55;
			
			sound stop $sound;
			
			worldmsg WE_SPELL_SYNC_END #INVALID_GOID #OWNER_GOID #TARGET_GOID;
	]];
}

