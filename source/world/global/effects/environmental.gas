			if ( #LODFI > 0.66 )
			{
				sfx create fire #TARGET 
					"count(2)offset(0,-.2,0)scale(2)color0(.05,.05,.05)color1(0,0,0)
					velocity(0,0,0)accel(0,0,0)min_displace(-.02)max_displace(.02)
					max_radius(.02)";
