			set $glow #POP;
			sfx start $glow;
			
			if ( #LODFI > 0.33 )
			{
				sfx create cylinder #TARGET 
					"alpha(.3)hp0(-.09,-.09,0)hp1(-.35,-.35,0)rp0(.202,.202,0)
					rp1(.164,.164,0)color0(1,1,.6)segments(4)bone_orient()
					rotate(90,45,0)";
				sfx start #POP;
