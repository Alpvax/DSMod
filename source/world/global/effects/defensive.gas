			sfx create cylinder #TARGET
				"color0($color)texture($texture)spin(-3)rp0(1.85,.5,-3)
				hp0(0,2,6)hp1(0,0,0)tin(.50)tout(.3)dur(.8)abs()offset(0,-.65,0)";
				
					
			sfx create cylinder #TARGET
				"color0($color)texture($texture)spin(3)rp0(1.85,.5,-3)
